    <!-- ZIP Code Checker Section - Moved to bottom -->
    <section id="zip-checker-section" class="py-12 md:py-16 bg-white">
        <div class="container mx-auto px-4">
            <div class="max-w-2xl mx-auto text-center bg-gray-50 p-8 rounded-lg shadow-lg">
                <h2 class="text-2xl md:text-3xl font-bold text-aj-dark mb-3">Do We Serve <span class="text-aj-orange">Your Area?</span></h2>
                <p class="text-gray-600 mb-6">Enter your 5-digit ZIP code to quickly find out if you're within our service range.</p>
                <div class="zip-search-form flex max-w-md mx-auto">
                    <input type="text" id="zip-search-input" placeholder="Enter your ZIP code" maxlength="5" pattern="[0-9]{5}" class="flex-grow p-3 rounded-l-md text-lg focus:ring-aj-orange focus:border-aj-orange">
                    <button id="check-zip-button" class="bg-aj-orange text-white font-semibold p-3 px-6 rounded-r-md text-lg">
                        <i class="fas fa-paper-plane mr-1"></i> Check
                    </button>
                </div>
                <div id="zip-search-result" class="mt-4 text-md min-h-[2em]">
                    <!-- Results will be displayed here -->
                </div>
            </div>
        </div>
    </section>

    <!-- Back to top button -->
    <button id="back-to-top" aria-label="Back to Top">
        <i class="fas fa-chevron-up"></i>
    </button>

    <!-- Assume footer is loaded by include-html.js -->
    <!-- Example: <div id="footer-placeholder"></div> -->
          
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Mobile menu (assuming elements are in header include)
            const menuToggle = document.getElementById('menu-toggle');
            const closeMenu = document.getElementById('close-menu');
            const mobileMenu = document.getElementById('mobile-menu');
            const menuOverlay = document.getElementById('menu-overlay');

            if (menuToggle && mobileMenu && menuOverlay) {
                menuToggle.addEventListener('click', () => {
                    mobileMenu.classList.add('active');
                    menuOverlay.classList.add('active');
                    document.body.style.overflow = 'hidden';
                });

                const hideMenu = () => {
                    mobileMenu.classList.remove('active');
                    menuOverlay.classList.remove('active');
                    document.body.style.overflow = '';
                };

                if (closeMenu) closeMenu.addEventListener('click', hideMenu);
                menuOverlay.addEventListener('click', hideMenu);
            }

            // New Tab functionality
            const areaTabButtons = document.querySelectorAll('.area-tab-button');
            const areaContents = document.querySelectorAll('.service-area-content');
            const sidebarContainer = document.getElementById('region-navigation-sidebar');

            function updateSidebar(activeAreaId) {
                sidebarContainer.innerHTML = '<h3 class="text-xl font-bold text-aj-dark mb-4">Quick Navigation</h3>'; // Reset sidebar
                const activeContent = document.getElementById(activeAreaId);
                if (!activeContent) return;

                const regionDiv = activeContent.querySelector('div[data-region-name]');
                if (regionDiv) {
                    const regionName = regionDiv.dataset.regionName;
                    const regionIcon = regionDiv.dataset.regionIcon;
                    
                    const regionHeader = document.createElement('a');
                    regionHeader.href = `#${regionDiv.id}`;
                    regionHeader.innerHTML = `<i class="${regionIcon} mr-2"></i>${regionName}`;
                    regionHeader.classList.add('text-lg', 'font-semibold', 'mb-2', 'block', 'text-aj-orange');
                    // sidebarContainer.appendChild(regionHeader); // Decided against this as main region is clear
                }

                const countyBlocks = activeContent.querySelectorAll('.county-block');
                countyBlocks.forEach((block, index) => {
                    const countyName = block.dataset.countyName;
                    const countyId = block.id;
                    const link = document.createElement('a');
                    link.href = `#${countyId}`;
                    link.textContent = countyName;
                    if (index === 0) link.classList.add('active'); // Make first county active by default
                    
                    // Smooth scroll for sidebar links
                    link.addEventListener('click', function(e) {
                        e.preventDefault();
                        document.querySelector(this.getAttribute('href')).scrollIntoView({
                            behavior: 'smooth',
                            block: 'start' 
                        });
                        // Update active state in sidebar
                        sidebarContainer.querySelectorAll('a').forEach(a => a.classList.remove('active'));
                        this.classList.add('active');
                    });
                    sidebarContainer.appendChild(link);
                });
            }
            
            areaTabButtons.forEach(button => {
                button.addEventListener('click', () => {
                    areaTabButtons.forEach(btn => btn.classList.remove('active'));
                    button.classList.add('active');
                    
                    const targetArea = button.dataset.area;
                    areaContents.forEach(content => {
                        content.classList.remove('active');
                        if (content.id === `${targetArea}-content`) {
                            content.classList.add('active');
                            updateSidebar(content.id);
                        }
                    });
                });
            });

            // Initial sidebar population for the default active tab
            const initialActiveTab = document.querySelector('.area-tab-button.active');
            if (initialActiveTab) {
                const initialAreaId = `${initialActiveTab.dataset.area}-content`;
                updateSidebar(initialAreaId);
            }
            
            // Lazy loading for images
            const lazyImages = document.querySelectorAll('.lazy-load');
            const lazyLoadObserver = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        const img = entry.target;
                        img.src = img.dataset.src;
                        img.classList.add('loaded');
                        observer.unobserve(img);
                    }
                });
            }, { rootMargin: '100px 0px', threshold: 0.01 });
            lazyImages.forEach(image => lazyLoadObserver.observe(image));
            
            // ZIP code search functionality
            const zipInput = document.getElementById('zip-search-input');
            const checkZipBtn = document.getElementById('check-zip-button');
            const zipResult = document.getElementById('zip-search-result');
            
            const serviceZips = { // These would ideally come from a more dynamic source
                virginia: ['22030', '22031', '22032', '22033', '22034', '22035', '22036', '22037', '22038', '22039', '22042', '22043', '22044', '22046', '22066', '22101', '22102', '22180', '22181', '22182', '22183', '22124', '22003', '22015'], // Fairfax, Arlington etc.
                dc: ['20001', '20002', '20003', '20004', '20005', '20006', '20007', '20008', '20009', '20010', '20011', '20012', '20015', '20016', '20017', '20018', '20019', '20020'],
                maryland: ['20814', '20815', '20816', '20817', '20850', '20851', '20852', '20853', '20854', '20895', '20901', '20902', '20903', '20904', '20705', '20706', '20715', '20716', '20770', '20742'] // Montgomery, PG etc.
            };
            
            if (checkZipBtn && zipInput && zipResult) {
                checkZipBtn.addEventListener('click', () => {
                    const zip = zipInput.value.trim();
                    zipResult.innerHTML = ''; // Clear previous result
                    
                    if (/^\d{5}$/.test(zip)) {
                        let found = false;
                        let region = '';
                        
                        for (const [key, zips] of Object.entries(serviceZips)) {
                            if (zips.includes(zip)) {
                                found = true;
                                region = key.charAt(0).toUpperCase() + key.slice(1);
                                break;
                            }
                        }
                        
                        if (found) {
                            zipResult.innerHTML = `<div class="text-green-600 font-semibold"><i class="fas fa-check-circle mr-1"></i> Great news! We serve ZIP code ${zip} in ${region}. <a href="/estimate.html" class="text-aj-orange hover:underline">Request an estimate now!</a></div>`;
                        } else {
                            zipResult.innerHTML = `<div class="text-yellow-700 bg-yellow-100 border border-yellow-300 p-3 rounded-md"><i class="fas fa-info-circle mr-1"></i> We may be able to service ZIP code ${zip}. Please <a href="/estimate.html" class="text-aj-orange font-semibold hover:underline">request an estimate</a> or call us at <a href="tel:7039970026" class="text-aj-orange font-semibold hover:underline">703-997-0026</a> to confirm.</div>`;
                        }
                    } else {
                        zipResult.innerHTML = `<div class="text-red-600 font-semibold"><i class="fas fa-exclamation-circle mr-1"></i> Please enter a valid 5-digit ZIP code.</div>`;
                    }
                });
                
                zipInput.addEventListener('keypress', (e) => {
                    if (e.key === 'Enter') {
                        checkZipBtn.click();
                    }
                });
            }

            // Chat Widget (assuming elements are in footer include or loaded elsewhere)
            const chatToggle = document.getElementById('chat-toggle-button'); // You'll need a button with this ID
            const chatPopup = document.querySelector('.chat-popup');
            const closeChat = document.getElementById('close-chat-button'); // You'll need a button with this ID in chat header
            const chatInputEl = document.querySelector('.chat-input input');
            const sendChatBtn = document.querySelector('.chat-input button');
            const chatMessages = document.querySelector('.chat-messages');

            if (chatToggle && chatPopup) {
                chatToggle.addEventListener('click', () => {
                    chatPopup.style.display = chatPopup.style.display === 'flex' ? 'none' : 'flex';
                });
            }
            if (closeChat && chatPopup) {
                closeChat.addEventListener('click', () => {
                    chatPopup.style.display = 'none';
                });
            }
            if (sendChatBtn && chatInputEl && chatMessages) {
                 const addChatMessage = (text, sender = 'user') => {
                    const messageDiv = document.createElement('div');
                    messageDiv.classList.add('mb-2', 'clear-both', 'max-w-[80%]');
                    const p = document.createElement('p');
                    p.classList.add('rounded-lg', 'py-2', 'px-3', 'inline-block', 'text-sm', 'break-words');
                    if (sender === 'user') {
                        p.classList.add('bg-aj-orange', 'text-white', 'float-right');
                        messageDiv.classList.add('ml-auto');
                    } else {
                        p.classList.add('bg-gray-200', 'text-gray-800', 'float-left');
                         messageDiv.classList.add('mr-auto');
                    }
                    p.textContent = text;
                    messageDiv.appendChild(p);
                    chatMessages.appendChild(messageDiv);
                    chatMessages.scrollTop = chatMessages.scrollHeight;
                };
                sendChatBtn.addEventListener('click', () => {
                    const message = chatInputEl.value.trim();
                    if (message) {
                        addChatMessage(message, 'user');
                        chatInputEl.value = '';
                        setTimeout(() => {
                            addChatMessage("Thanks for your message! An agent will be with you shortly. For immediate help, call 703-997-0026.", 'agent');
                        }, 1000);
                    }
                });
                chatInputEl.addEventListener('keypress', (e) => {
                    if (e.key === 'Enter') sendChatBtn.click();
                });
            }
            
            // Back to top functionality
            const backToTopButton = document.getElementById('back-to-top');
            if (backToTopButton) {
                window.addEventListener('scroll', () => {
                    if (window.pageYOffset > 300) {
                        backToTopButton.classList.add('visible');
                    } else {
                        backToTopButton.classList.remove('visible');
                    }
                });
                backToTopButton.addEventListener('click', () => {
                    window.scrollTo({ top: 0, behavior: 'smooth' });
                });
            }
            
            // Copyright Year (assuming element with ID 'copyright-year' is in footer include)
            const copyrightYearElement = document.getElementById('copyright-year');
            if (copyrightYearElement) {
                copyrightYearElement.textContent = new Date().getFullYear();
            }

            // Sticky sidebar active link on scroll
            const countySectionsForScroll = document.querySelectorAll('.service-area-content.active .county-block');
            const sidebarLinks = sidebarContainer.querySelectorAll('a');

            const scrollObserver = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting && entry.intersectionRatio >= 0.5) {
                        const id = entry.target.getAttribute('id');
                        sidebarLinks.forEach(link => {
                            link.classList.remove('active');
                            if(link.getAttribute('href') === `#${id}`) {
                                link.classList.add('active');
                            }
                        });
                    }
                });
            }, { threshold: 0.5, rootMargin: "-50% 0px -50% 0px" }); // Adjust rootMargin to trigger when section is in middle of viewport

             function observeActiveTabSections() {
                document.querySelectorAll('.service-area-content.active .county-block').forEach(section => {
                    scrollObserver.observe(section);
                });
            }
            observeActiveTabSections(); // Initial observation
            // Re-observe when tabs change
             areaTabButtons.forEach(button => {
                button.addEventListener('click', () => {
                    setTimeout(observeActiveTabSections, 10); // slight delay to ensure content is active
                });
            });


        });
    </script>
    
    <!-- Housecall Pro Booking Widget Script -->
    <script async src="https://online-booking.housecallpro.com/script.js?token=4bcbc2f7231b4633abc97173fe9db382&orgName=AJ-Long-Electric"></script>
    <!-- Include HTML components (header, footer) -->
    <script src="/js/include-html.js?v=.1746952695."></script>
</body>
